<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<!-- 实体类和表的映射 -->
<class name="cn.com.bean.Result" table="result" catalog="psy" >
<id name="rid" column="rid"  type="java.lang.Integer">
<generator class="identity"></generator>
</id>
<property name="id" type="java.lang.Integer" length="255"></property>
<property name="rtitle" type="java.lang.String" length="255"></property>
<property name="rdesp" type="java.lang.String" length="255"></property>
<property name="fsource" type="java.lang.Integer" length="255"></property>
<property name="tsource" type="java.lang.Integer" length="255"></property>
</class>
</hibernate-mapping>
