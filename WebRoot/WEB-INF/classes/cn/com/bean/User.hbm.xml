<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<!-- 实体类和表的映射 -->
<class name="cn.com.bean.User" table="User" catalog="psy" >
<id name="uid" column="uid"  type="java.lang.Integer">
<generator class="identity"></generator>
</id>
<property name="username" type="java.lang.String" length="255"></property>
<property name="password" type="java.lang.String" length="255"></property>
<property name="logo" type="java.lang.String" length="255"></property>
</class>
</hibernate-mapping>
